{
  "routes": [
    { "route": "/Client1/*", "allowedRoles": ["client1"] },
    { "route": "/Client2/*", "allowedRoles": ["client2"] }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/access-denied.html"
    }
  }
}